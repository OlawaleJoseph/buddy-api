{
  "name": "buddy-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "coverage": "npm run pretest && cross-env NODE_ENV=test jest --coverage && coveralls < coverage/lcov.info",
    "dev": "nodemon --exec babel-node src/index.js",
    "lint": "./node_modules/.bin/eslint src/",
    "lint:fix": "./node_modules/.bin/eslint src/ --fix",
    "resetDb": "sequelize db:migrate:undo:all && npm run migrate",
    "migrate": "sequelize db:migrate",
    "seed": "sequelize db:seed:all",
    "start": "node ./dist/index.js",
    "test": "cross-env NODE_ENV=test jest --config ./jest.config.json --timeout=50000",
    "pretest": "cross-env NODE_ENV=test npm run undo && cross-env NODE_ENV=test sequelize db:migrate",
    "test:watch": "npm run test --watch",
    "undo": "sequelize db:migrate:undo:all"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.12.10",
    "babel-node": "0.0.1-security",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.19.0",
    "concurrently": "^5.3.0",
    "core-js": "^3.8.3",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.9.2",
    "faker": "^5.3.1",
    "helmet": "^4.4.1",
    "joi": "^17.3.0",
    "jsonwebtoken": "^8.5.1",
    "make-runnable": "^1.3.8",
    "morgan": "^1.10.0",
    "mysql2": "^2.2.5",
    "regenerator-runtime": "^0.13.7",
    "sequelize": "^6.5.0",
    "sequelize-cli": "^6.2.0",
    "swagger-jsdoc": "^6.0.1",
    "swagger-ui-express": "^4.1.6"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "coveralls": "^3.1.0",
    "eslint": "^7.18.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "supertest": "^6.1.3"
  }
}
